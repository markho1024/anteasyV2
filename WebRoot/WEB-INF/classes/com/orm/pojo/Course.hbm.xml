<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.orm.pojo.Course" table="Course">
		<id name="courseID" column="course_id" type="long">
			<generator class="assigned"></generator>
		</id>
		<property name="courseName" column="course_name" type="string"></property>
        
        <property name="collegeID" column="college_id" type="long"></property>
        <property name="courseKind" column="course_kind" type="string"></property>

		<property name="remark" column="remark" type="string"></property>
		<property name="createDate" column="create_date" type="date"></property>
		<property name="status" column="status" type="string"></property>
	</class>
</hibernate-mapping>
