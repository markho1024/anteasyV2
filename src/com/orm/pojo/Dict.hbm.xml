<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.orm.pojo.Dict" table="Dict">
		<id name="dictID" column="dict_id" type="long" >
			<generator class="increment"></generator>
		</id>

		<property name="dictType" column="dict_type" type="string"></property>
		<property name="dictCode" column="dict_code" type="string"></property>
		<property name="dictName" column="dict_name" type="string"></property>
		<property name="remark" column="remark" type="string"></property>
		<property name="createDate" column="create_date" type="date"></property>
		<property name="status" column="status" type="string"></property>
	</class>
</hibernate-mapping>