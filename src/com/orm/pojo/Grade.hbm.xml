<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.orm.pojo.Grade" table="Grade">
	    <id name="gradeID" column="grade_id" type="long" >
	      <generator class="assigned"></generator>
	    </id>
	    
		<property name="gradeName" column="grade_name" type="string"></property>
		<property name="studentType" column="student_type" type="string"></property>
	</class>
</hibernate-mapping>
