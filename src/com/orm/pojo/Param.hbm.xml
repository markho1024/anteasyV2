<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.orm.pojo.Param" table="Param">
		<id name="paramID" column="param_id" type="long">
			<generator class="assigned"></generator>
		</id>
		<property name="paramName" column="param_name" type="string"></property>
		<property name="paramValue" column="param_Value" type="string"></property>

	</class>
</hibernate-mapping>
