<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.orm.pojo.Plan" table="Plan">
		<id name="planID" column="plan_id" type="long">
			<generator class="increment"></generator>
		</id>
		<property name="majorID" column="major_id" type="long"></property>
		<property name="gradeID" column="grade_id" type="long"></property>
		<property name="studentType" column="student_type" type="string"></property>
		<property name="courseID" column="course_id" type="long"></property>
		
		<property name="remark" column="remark" type="string"></property>
		<property name="createDate" column="create_date" type="date"></property>
		<property name="status" column="status" type="string"></property>
	</class>
</hibernate-mapping>
